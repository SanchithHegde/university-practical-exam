{
  "dependencies": {
    "@tailwindcss/forms": "^0.3.3",
    "autoprefixer": "^10.2.6",
    "postcss": "^8.3.3",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^2.1.4"
  },
  "scripts": {
    "build": "postcss css/tailwind.css -o ../practical_exam/exam_scheduling/static/exam_scheduling/css/tailwind-generated.css",
    "watch": "npm-watch"
  },
  "watch": {
    "build": {
      "patterns": [
        "src",
        "css"
      ],
      "extensions": "css,html",
      "quiet": false,
      "delay": 1000
    }
  },
  "devDependencies": {
    "npm-watch": "^0.10.0"
  }
}
